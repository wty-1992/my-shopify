    } 
    color: $color-button1-active;
  }
}
/*====================== image loading =======================*/
.lazyloading:not(iframe) {
  opacity: 1;
  transition: 600ms opacity;
  background-image:url({{settings.mask_loading_image | img_url: 'medium'}});
  background-repeat:no-repeat;
  background-position:center;
  height: auto;
} 
input[type='search']::-webkit-search-cancel-button {
  position: relative;
  left: -$gutter-site / 2;
}  

