        {% for link in menus[section.settings.menu_1].links %}
        <li>
          <a{%if link.title == 'Send a Query' %} onclick="Dialog.open('#footer-send-query')"  href="javasript:;"{% else %}  href="{{ link.url }}" {% endif %}>{{ link.title }}</a> 
