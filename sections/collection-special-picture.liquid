        position: relative;
      }
      .bg_style_link{
        position: absolute;
        width: 100%;
        height: {{imgHeight | divided_by: imgWidth | minus: 6 }}vw;
        display: block;
        top: 0;
        left: 0;  
        z-index:2;
      }
      @media(max-width:768px){
        .collection-special-picture{
          background: url({{ block.settings.mb_image | img_url:'master' }}) no-repeat top left;     
          background-size: contain;
          padding-top:{{imgHeightMB | divided_by: imgWidthMB | divided_by: 1.3 }}vw;
        }
        .bg_style_link{
          height: {{imgHeightMB | divided_by: imgWidthMB | divided_by: 1.3 }}vw;
        }
