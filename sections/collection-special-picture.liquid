<style>
  .collection-banner-pc{display:block;margin: auto;}
  .collection-banner-mb{display:none;margin: auto;}
  .collection-banner{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 0;
    flex-flow: wrap;
  }
  .collection-special-picture .collection-banner{
    padding: 0 0 10px;
  }
  .collection-banner.collection-banner-one,.collection-banner.collection-banner-three,.collection-banner.collection-banner-four{
    width: 1200px;
    margin-left: auto;
    margin-right: auto;
  }
  .collection-special-picture .shopby_title {
    text-align: center;  
    color: #000;
    margin:0;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-flow: column;
    align-items: center;
    justify-content: center;
  }
  .collection-special-picture .shopby_title span{
    font-size: 20px;
    font-weight: 600;
    line-height: 1;
    display: block;
  }
  .collection-special-picture .shopby_title p{
    font-size: 16px;
    margin-bottom: 0;
  }
  .collection-banner.collection-banner-one .collection-banner-row{width:calc(50% - 5px);}
  .collection-banner.collection-banner-three .collection-banner-row{width:calc(33% - 5px);}
  .collection-banner.collection-banner-four .collection-banner-row{width:calc(25% - 5px);}
  .collection-banner .collection-banner-row{
    overflow: hidden;
    position: relative;
    transition: all .3s;
  }
  .collection-banner .collection-banner-row img{width:100%;transition: all .3s;}
  .collection-banner a{
    display: block;   
    position: relative;
  }
  @media(max-width:1200px){
    .collection-banner.collection-banner-one,.collection-banner.collection-banner-three,.collection-banner.collection-banner-four{width:100%;}
    .collection-banner.collection-banner-four .collection-banner-row{width:calc(50% - 5px);}
    .collection-banner-pc{display:none;}
    .collection-banner-mb{display:block;}
  }
  {% if handle == 'necklaces' or  handle == 'bracelets' %}
    .collection-special-picture .shopby_title {
      height: 70px;
      background: rgba(255,255,255,.4);
    }
    @media(max-width:768px){
      .collection-special-picture .shopby_title {
        height: 45px;       
      }
      .collection-special-picture .collection-banner{
        padding: 0 6px 6px;
      }
      .collection-special-picture .shopby_title span{
        font-size: 12px;
      }
      .collection-special-picture .shopby_title p{
        font-size: 12px;
      }
      .collection-banner-three .shopby_title p{
        display: none;
      }
      .collection-banner.collection-banner-one .collection-banner-row{width:calc(50% - 3px);}
      .collection-banner.collection-banner-three .collection-banner-row{width:calc(33% - 3px);}
    }      
  {% endif %}  
  {% if handle == 'apparel' or handle == 'occasions' or handle == 'recipients' %}
  	.collection-special-picture .shopby_title span{
      font-size: 40px;
      color: #fff;
      text-shadow: 0 0 15px rgb(0 0 0 / 90%);
      padding: 0 40px;
      line-height: 1.3;
      font-family: OpenSans-Light;
    }
    @media(max-width:768px){
 	  .collection-banner-row{
        width: 100% !important;
      }
      .collection-special-picture .shopby_title{
        position: unset;
      }
      .collection-special-picture .shopby_title span{
        font-size: 18px;
        color: #000;
        display: flex;
    	flex-flow: column;
        margin-bottom: 25px;
        margin-top: 15px;
        text-shadow: none;
        align-items: center;
      }
      .collection-special-picture .shopby_title span:after{
        content:"{{'common.top5.buy_now' | t }}";
        background: #da004e;
        padding: 8px 38px;
        color: #fff;
        text-transform: uppercase;
        display: inline-block;
        font-size: 12px;
    	margin-top: 12px;
      }
      .collection-banner-swiper{
        margin-bottom:0;
      }
    }    
    {% if handle == 'recipients' %}
      .collection-special-picture .shopby_title span {
        font-weight: 100;
      }
    {% endif %}
  {% endif %}
  {% if handle == 'charms' or handle == 'rings' or handle == 'watches' %}
    .collection-special-picture .shopby_title{
 	  height: 35%;
    }
  	.collection-special-picture .shopby_title span{
      font-size: 16px;
      font-weight: 100;
    }
    .collection-banner-swiper{
      margin-bottom:0;
    }
    @media(max-width:768px){
      .collection-special-picture .collection-banner{
        padding: 0 5px 0;
      }
      .collection-banner-row{
        width: calc(50% - 2.5px) !important;
        margin-bottom: 5px;
      }
      .collection-special-picture .shopby_title span {
        font-size: 13px;       
      }
    }
    {% if handle == 'watches' %}
  	  .picture_box{
        background-image: linear-gradient(#f5e76c, #fff);
      }   
      @media(max-width:768px){
        .collection-banner-row{
          width: 50% !important;
        }
        .collection-banner:first-child .collection-banner-row:first-child{
          width: 100% !important;
        }        
        .collection-banner-row{
          margin-bottom: 0;
        }
        .collection-special-picture .collection-banner{
          padding: 0;
        }
        .collection-banner .collection-banner-row img{
          max-height:172px;
        }
      }
    {% endif %}
  {% endif %}
</style>
