<div class="footer-mb-section">  
  {%- comment -%}footer pionter{%- endcomment -%}  
  <div class="footer">
    <div class=" footer-inpoint">
      <div class=""> 
        <ul class="flex justify-content-flexstart">
          {% for block in section.blocks %}
          {% case block.type %}
          {% when 'footer_inpoint_pc' %}
          <li>
            <div class="footer-payments">             
              <a href= "{% if forloop.index==8 %}{{ block.settings.footer_inpointurl}}{% else %}javascript:;{% endif %}"  > <img src="{{block.settings.footer_inpoint  | img_url: 'master'}}"></a>
            </div>
            <div class="policy-text">
              {{ block.settings.footer_icon_text }}
            </div>
          </li>
          {% endcase %}
          {% endfor %}
        </ul>
      </div>
     </div>
   </div>
  {%- comment -%}footer pionter end{%- endcomment -%}
  {%- comment -%}
  footer cms page and emmail start by jenny 2018-12-8
  {%- endcomment -%}
  
  <div class="footer-middle-block inputemail">
    <div class="footer-content">
      
      {%- assign formId = 'ContactFooter-mb' -%}
        {% form 'customer', id: formId  %}
          
          <input type="hidden" name="contact[tags]" value="newsletter">
          <div class="input-box flex">
            <div class="input-email">
              <input type="email"
                name="contact[email]"
                id="newsletter_footer-mb"
                class="input-text required-entry validate-email {% if form.errors %} input--error{% endif %}"
                value="{% if customer %}{{ customer.email }}{% endif %}"
                placeholder="{{ 'footer.newsletter_form.newsletter_placeholder' | t }}"
                autocorrect="off"
                autocapitalize="off"
                {% if form.errors %} aria-invalid="true" aria-describedby="{{ formId }}-email-error"{% endif %}>
              <div class="input-box_email">
                <button type="submit" title="Subscribe" class="button">
                  
                </button>
              <img src="{{ 'email.png' | asset_url }}">
              </div>
            </div>
            {% comment %}
            <button type="submit" title="Subscribe" class="button">
                <span> {{ 'footer.newsletter_form.submit' | t }} </span>
            </button>
            {% endcomment %}
          </div>
      	
          <div class="errors" style="display:none;" id="custom_subscribe_error_mb"><ul><li>
           </li></ul></div>
          {{ form.errors | default_errors }}
      
          <div class="footer-policy">
            
            <input type="checkbox" id="newsletter_policy_confirm-mb" class="newsletter_policy_confirm">{{ 'footer.newsletter_form.policy_text' | t }}
            &nbsp;<a href="/pages/privacy-policy" target="_blank"><span class="policy-span text-u"> {{ 'footer.newsletter_form.policy' | t }}</span></a>
          </div>

          {% if form.posted_successfully? %}
            <p class="form-success">{{ 'footer.newsletter_form.confirmation' | t }}</p>
          {% endif %}

          <script>
            $("#ContactFooter-mb").on("submit", function(){
                if (!$("#newsletter_policy_confirm-mb").is(':checked')){
                    $("#custom_subscribe_error_mb").show();
                    $("#custom_subscribe_error_mb").find("ul > li").text("{{ 'footer.newsletter_form.required_Privacy' | t }}")
                    return false;
                }
                Mask.show();
            });
          </script>

        {% endform %}

    </div>
  </div>
  
  {% comment %} society icon {% endcomment %}
          <div class="footer-society">
            <div class="container"> 
              <ul class="flex justify-content-flexstart">
                {% for block in section.blocks %}
                {% case block.type %}
                {% when 'footer_icon_pc' %}
                {% if block.settings.footer_icon_url %}
                <li>
                  <a href="{{block.settings.footer_icon_url }}" target="_blank">
                    <img src="{{block.settings.footer_icon  | img_url: 'master'}}">
                  </a>
                </li>
                {% endif %}
                {% endcase %}
                {% endfor %}
              </ul>
            </div>
          </div>
  
  
  <div class="page-width flex cmspage">  
  <div id="footeraccordion">
    <div class="footer-content">
      <div class="footer-content-header" id="headingOne"> 
        <a data-toggle="collapse" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
          <h2>{{ section.settings.menu_1_text }}</h2>
        </a> 
      </div> 
      <div id="collapseOne" class="collapse block-content" data-parent="#footeraccordion">
          <ul class="links">
            {% for link in menus[section.settings.menu_1].links %}
            <li>
              <a{%if link.title == 'Enviar una Consulta' %} onclick="Dialog.open('#footer-send-query')"  href="javasript:;"{% else %}  href="{{ link.url }}" {% endif %}>{{ link.title }}</a> 
            </li>  
            {% endfor %} 
          </ul>
      </div>
    </div>
    <div class="footer-content">
      <div class="footer-content-header" id="headingTwo"> 
        <a data-toggle="collapse" href="#collapseTwo" aria-expanded="false" aria-controls="collapseOne">
          <h2>{{ section.settings.menu_2_text }}</h2>
        </a> 
      </div> 
      <div id="collapseTwo" class="collapse block-content" data-parent="#footeraccordion">
        <ul class="links">
         {% for link in menus[section.settings.menu_2].links %}
         <li data-title="{{ link.title }}"><a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a></li>
         {% endfor %}
        </ul>
      </div>
    </div>
  	 <div class="footer-content">
      <div class="footer-content-header" id="headingThree"> 
        <a data-toggle="collapse" href="#collapseThree" aria-expanded="false" aria-controls="collapseOne">
          <h2>{{ section.settings.menu_3_text }}</h2>
        </a> 
      </div> 
      <div id="collapseThree" class="collapse block-content" data-parent="#footeraccordion">
        <ul class="links">
           {% for link in menus[section.settings.menu_3].links %}

            <li> <a{%if link.title == 'Submit an Inquiry' %} onclick="Dialog.open('#footer-send-query',{'cssClass':['mb-footer-send-query']})"  href="javasript:;"{% else %}  href="{{ link.url }}" {% endif %}>{{ link.title }}</a></li>
            {% endfor %}
            {{ section.settings.footer_nav_bottom_html }}
        </ul>
      </div>
    </div>

  </div>
 </div>
  {% if section.settings.app_logo_mb  %}
