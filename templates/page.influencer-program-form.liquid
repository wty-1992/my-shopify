}
.form-group.name.col-xs-12 li{
  display: inline-block;
}
.top li{ list-style:none; text-align:center}
.floating-labels li{ list-style:none}
.floating-labels input{ 
  height: 50px;
  line-height: 50px;
  padding: 0 0 0 20px;
  width: 100%;
}
input[type="checkbox"],input[type="radio"]{width: auto;height: auto;}
.floating-labels label, h2 {
  display: block;
  font-size: 16px;
  color: #000;
  font-weight: 600;
}
.checkbox label, .gender label{ 
  display:inline-block; 
  font-size: 14px;
  color: #666;
  font-weight: 200;
}
.box select {
  background: #fff;
  border: 1px solid #ccc;
  padding: 2px 8px;
  height: 50px;
  color: #777;
  width: 100%;
  font-size: 12px;
  border-radius: 0;
}
input[type=radio] {
  margin: -3px 0 0;
}
sup{ color: #e60044;}
.checkbox+.checkbox, .radio+.radio, .checkbox{
  margin-top: 0px;
  position:relative
}
.lastname{ margin-left:20px;}
#defaultForm h2{ 
  margin-bottom:10px;
  text-align: left;
  margin-left: 0;
}
.form-group{ margin-bottom:20px;}
#submitBtn{ 
  padding: 15px 40px;
  color: #fff;
  background: #e60044; 
  border:0 solid #fff;
}
#otherMainStiteLabel{ 
  position:absolute; 
  left:0;
  top: 20px;
}
.cms-page-view .main-container {
  padding: 0;
}

@media (max-width: 640px){
  .top img{ width:100%;} 
  .top {
    margin: 25px 0;
    float: left;
    width: 100%;
  }
}
.pc-aboutusimg{display:block}
.mb-aboutusimg{display:none} 
@media (max-width: 768px){
  .mb-aboutusimg{display:block}
  .pc-aboutusimg{display:none}
  .form-group.name.col-xs-12 li{    
    width: calc(50% - 8px);
  }
  .lastname {
    margin-left: 10px;
  }
  .floating-labels input[type="text"], .floating-labels input[type="password"], .floating-labels input[type="email"]{
    padding-left:15px;
  }
  .checkbox label, .gender label{
    text-transform: capitalize;
  }
  .checkbox label input, .gender label input{
    margin-right: 6px;
  }
  .form-group.col-xs-12 .col-xs-12 {
    display: flex;
    flex-flow: wrap;
  }
  .form-group.col-xs-12 .col-xs-12 .checkbox.col-xs-6{
    width: 50%;
  }
}
.validation-advice {
  clear: both;
  min-height: 13px;
  margin: 3px 0 0;
  padding-left: 10px;
  font-size: 11px;
  line-height: 13px;
  color: #eb340a;
  position: relative;
  text-align: left;
}
