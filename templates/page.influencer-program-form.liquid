<script type="text/javascript">  
  function isOtherMainStite() {
	var mainSiteType = jQuery("input:checkbox[name='mainSiteType[]']:checked");
	var val = '';
	mainSiteType.each(function(){
      val = jQuery(this).val();
      if(val == 'Other') {
        jQuery("#otherMainStiteLabel").show();
        jQuery("#otherMainStite").addClass('required-entry');
      } else {
        jQuery("#otherMainStiteLabel").hide();
        jQuery("#otherMainStite").removeClass('required-entry');
      }
	});
  }
  
  function subform(){
    var myreg = /^[\w-]+(\.[\w-]+)*@[\w-]+(\.[\w-]+)+$/;
    var urlreg = /^(http(s)?:\/\/)?([\w-]+\.)+[\w-]+(\/[\w- .\/?%&=]*)?$/;
    var numreg = /^[\d]+$/;
    var flag = true;
    var data = {};
    $('.validation-advice').remove();
    $('#defaultForm input[type=text]').each(function(){
      if($(this).val() == '' && $(this).is(':visible')){
        var varien = '<div class="validation-advice" id="advice-required-entry-email" style="">{{'influencer.form.msg_required' | t }}</div>';
      	$(this).after(varien);
        flag = false;
      }else if($(this).attr('name') == 'email' && !myreg.test($(this).val())){
        var varien = '<div class="validation-advice" id="advice-required-entry-email" style="">{{'influencer.form.msg_email_error' | t }}</div>';
      	$(this).after(varien);
        flag = false;
      }else if(($(this).attr('name') == 'mainSiteurl' || $(this).attr('name') == 'otherSiteUrl') && !urlreg.test($(this).val())){
        var varien = '<div class="validation-advice" id="advice-required-entry-email" style="">{{'influencer.form.msg_url_error' | t }}</div>';
      	$(this).after(varien);
        flag = false;
      }else if($(this).attr('name') == 'fansNum' && !numreg.test($(this).val())){
        var varien = '<div class="validation-advice" id="advice-required-entry-email" style="">{{'influencer.form.msg_num_error' | t }}</div>';
      	$(this).after(varien);
        flag = false;
      }else{
        //flag = true;
        data[$(this).attr('name')] = $(this).val();
      }
    })
    if(flag == false){
      return false;
    }
    var mainSiteType =[];
    $('input[name="mainSiteType[]"]:checked').each(function(){
      mainSiteType.push($(this).val());
    });
    data['mainSiteType'] = mainSiteType;
    data['gender'] = $("input[name='gender']:checked").val();
    data['country_id'] = $('#country').val();
    $("#submitBtn").attr("disabled", true);
  	$.ajax({
      type: 'POST',
      url: 'https://socks.soufeel.com/supertools/110/save.php',
      data: data,
      dataType: 'json',
      beforeSend: function(xhr) {
      },
      success: function(boxItem) {
        var msg = "{{'influencer.form.msg_success' | t }}";
        if(boxItem.msg == 'Your infomation has been received.'){
          msg = "{{'influencer.form.msg_repeat' | t }}";
        }
        $('.box').html(msg);
        $('.box').css({"text-align":"center","padding":"100px 20px"});
        $('.influencerimg').hide();
        $("#submitBtn").attr("disabled", false);
      },
      error: function(XMLHttpRequest, textStatus){
        console.log('card product to cart error');
      }
    });
  }
</script>