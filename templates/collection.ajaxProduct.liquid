      function formatSku(sku){
        var sku_val = sku;
        if(sku_val.indexOf('-') > -1){
          var zenSkuArr = sku_val.split('-');
          var regPos = /^\d+(\.\d+)?$/;
          zenSkuArr.forEach(function(item,i){
              if(!regPos.test(item)){
                  sku_val = item;
              }
          });
        }
        return sku_val;
      }

