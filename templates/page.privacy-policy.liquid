}/*
* @description: privacy-policy页面
* @author: sandy
* @update: sandy (2015-08-25)
*/
.fs20{font-size: 20px;}
.privacy {
    position: relative;
    width: 100%;
    height: 100%;
}
.privacy .splitlayout {
    position: relative;
    overflow-x: hidden;
    min-height: 100%;
    width: 100%;
}

/* Intro sides */
.side {
   /* margin-top:122px;*/
    position: fixed;
    top: 40px;
    z-index: 1;
    width: 50%;
    height: 100%;
    text-align: center;
    -webkit-backface-visibility: hidden;
}
.open-left .side,.open-right .side {
    cursor: default;
}
.overlay {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 499;
    visibility: hidden;
    width: 100%;
    height: 100%;
    opacity: 0;
}
.side-left .overlay {
    background: rgba(0,0,0,0.7);
}
.side-right .overlay {
    background: rgba(0,0,0,0.3);
}
.side-left {
    left: 0;
    background: #e60044;
    outline: 1px solid #09b8ae; /* avoid gap */
}
.side-right {
    right: 0;
    background: #fff;
    outline: 1px solid #fff; /* avoid gap */
}

/* Intro content, profile image and name, back button */
.privacy .intro .intro-content {
    position: absolute;
    top: 50%;
    left: 50%;
    padding: 0 1em;
    width: 50%;
    cursor: pointer;
    -webkit-transform: translateY(-50%) translateX(-50%);
    transform: translateY(-50%) translateX(-50%);
}
.privacy .intro .intro-content h1 > span {
    display: block;
    /*white-space: nowrap;*/
    word-wrap: break-word;
    word-break: normal;
    white-space: normal;
}
.privacy .intro .intro-content h1 > span:first-child {
    font-weight: 300;
    font-size: 1.4rem;
}
.privacy .intro .intro-content h1 > span:nth-child(2) {
    position: relative;
    margin-top: 0.5em;
    padding: 0.8em 0;
    text-transform: uppercase;
    letter-spacing: 1px;
    /*font-size: 0.75em;*/
    font-size:12px;
}
.privacy .back{
    position: fixed;
    top: 10em;
    z-index: 500;
    display: block;
    visibility: hidden;
    width: 50px;
    height: 50px;
    border-radius: 50%;
    color: #000;
    text-align: center;
    font-size: 22px;
    line-height: 44px;
    opacity: 0;
    pointer-events: none;
}
.mobile-layout .back { /* fixed positioning will make this not clickable after scrolling on some mobile devices */
   /* position: absolute;*/
}
.back-left {
    left: 12.5%;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
}
.back-right {
    right: 12.5%;
    -webkit-transform: translateX(50%);
    transform: translateX(50%);
    color: #fff;
}
.open-right .back-left,
.open-left .back-right {
    visibility: visible;
    opacity: 1;
    -webkit-transition-delay: 0.3s;
    transition-delay: 0.3s;
    pointer-events: auto;
}
.back:hover {
    color: #ddd;
}
/* privacy-pages */
.privacy-page {
    position: relative;
    top: 0;
    overflow: auto;
    min-height: 100%;
    width: 75%;
    height: auto;
    font-size: 12px;
    -webkit-backface-visibility: hidden;
}
.privacy-page strong {
    color:#000;
}
.privacy-page-right {
    left: 25%;
    outline: 5px solid #ecf0f1; /* avoid rounding gaps */
    -webkit-transform: translateX(100%);
    transform: translateX(100%);
}
.splitlayout.open-right {
    background: #fff;
}
.privacy-page-left {
    left: 0;
    outline: 5px solid #34495e; /* avoid rounding gaps */
    color: #fff;
    text-align: right;
    -webkit-transform: translateX(-100%);
    transform: translateX(-100%);
}
.splitlayout.open-left {
    background: #f7f8f9;
}

/* Inner privacy-page content */
.privacy-page-inner {
    background:#fff;
    padding: 2em 6em 2em 0;
}
.privacy-page-inner section {
    padding-bottom: 1em;
}
.privacy-page-inner h2 {
  margin: 0 0 1em 0;
  text-align:right;
  text-transform:uppercase;
  font-weight: 100;
  color: #000;
}
.privacy-page-inner h3{
  font-size: 15px;
  text-align: right;
  margin-right: 0;
}
.privacy-page-inner p {
    line-height:24px;
    margin:0;
    font-size: 12px;
    color: #666;
    
}
/* All transitions */
.side,
.privacy-page {
    -webkit-transition: -webkit-transform 0.6s;
    transition: transform 0.6s;
}
.overlay {
    -webkit-transition: opacity 0.6s, visibility 0.1s 0.6s;
    transition: opacity 0.6s, visibility 0.1s 0.6s;
}
.intro-content {
    text-transform:uppercase;
    -webkit-transition: -webkit-transform 0.6s, top 0.6s;
    transition: transform 0.6s, top 0.6s;
}
.intro-content h1,
.back {
    -webkit-transition: opacity 0.3s;
    transition: opacity 0.3s;
}
/* Open and close */
/* We need to set the position and overflow for the respective privacy-page scroll */
.reset-layout .privacy-page,
.splitlayout.open-right .privacy-page-left,
.splitlayout.open-left .privacy-page-right,
.splitlayout.close-right .privacy-page-left,
.splitlayout.close-left .privacy-page-right {
    position: absolute;
    overflow: hidden;
    height: 100%;
}
.splitlayout.open-right .privacy-page-right,
.splitlayout.open-left .privacy-page-left {
    position: relative;
    overflow: auto;
    height: auto;
}
.open-right .side-left .overlay,
.open-left .side-right .overlay {
    visibility: visible;
    opacity: 1;
    -webkit-transition: opacity 0.6s;
    transition: opacity 0.6s;
}
/* Right side open */
.open-right .side-left {
    -webkit-transform: translateX(-60%);
    transform: translateX(-60%);
}
.open-right .side-right { 
    -webkit-transform: translateX(-150%);
    transform: translateX(-150%);
} 
.open-right .side-right .intro-content {
    -webkit-transform: translateY(-50%) translateX(0%) scale(0.6);
    transform: translateY(-50%) translateX(0%) scale(0.6);
}
.open-right .privacy-page-right {
    -webkit-transform: translateX(0%);
    transform: translateX(0%);  z-index:0
}
/* Left side open */
.open-left .side-right {
    -webkit-transform: translateX(60%);
    transform: translateX(60%);
  z-index:0
}
.open-left .side-left { 
    -webkit-transform: translateX(150%);
    transform: translateX(150%);
} 
.open-left .side-left .intro-content {
    -webkit-transform: translateY(-50%) translateX(-100%) scale(0.6);
    transform: translateY(-50%) translateX(-100%) scale(0.6);
}
.open-left .codropsheader {
    opacity: 0;
    visibility: hidden;
    -webkit-transition: opacity 0.3s, visibility 0.1s 0.3s;
    transition: opacity 0.3s, visibility 0.1s 0.3s;
}
.open-left .privacy-page-left {
    -webkit-transform: translateX(0%);
    transform: translateX(0%);
}
